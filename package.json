{
  "name": "window-page",
  "version": "12.2.0",
  "description": "Route, setup, and build web pages",
  "main": "index.js",
  "scripts": {
    "test": "npm run prepare && mocha",
    "prepare": "postinstall"
  },
  "prepare": {
    "./index.js": "bundle --name=Page window-page.js"
  },
  "files": [
    "window-page.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kapouer/window-page.git"
  },
  "keywords": [
    "dom",
    "router",
    "history",
    "webcomponent",
    "prerender"
  ],
  "author": "Jérémy Lal <kapouer@melix.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kapouer/window-page/issues"
  },
  "devDependencies": {
    "@kapouer/eslint-config": "^1.5.1",
    "expect.js": "^0.3.1",
    "express": "^4.17.1",
    "express-dom": "^5.13.4",
    "gport": "^1.1.1",
    "levenlistdiff": "^1.0.1",
    "mocha": "^9.1.1",
    "postinstall": "^0.7.0",
    "postinstall-bundle": "^0.7.0",
    "request": "^2.88.2",
    "selenium-webdriver": "^3.0.1",
    "webcomponents.js": "^0.7.24",
    "webkitgtk": "^4.12.1",
    "window-agent": "^1.2.0"
  },
  "mocha": {
    "spec": "test/*.js",
    "exit": true
  },
  "eslintConfig": {
    "extends": "@kapouer/eslint-config",
    "ignorePatterns": [
      "window-page.js"
    ],
    "overrides": [
      {
        "files": [
          "src/*.js"
        ],
        "env": {
          "es6": true
        }
      },
      {
        "files": [
          "test/*.js"
        ],
        "env": {
          "mocha": true
        }
      },
      {
        "files": [
          "test/**/*.js"
        ],
        "globals": {
          "Page": true
        },
        "env": {
          "browser": true
        }
      }
    ]
  }
}
